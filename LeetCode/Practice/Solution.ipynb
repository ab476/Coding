   "source": [
    "from typing import List\n",
    "from collections import deque\n",
    "\n",
    "def maxSlidingWindow(nums: List[int], k: int) -> List[int]:\n",
    "    dq: deque[int] = deque()\n",
    "\n",
    "    for num in nums:\n",
    "        if dq and len(dq) >= k:\n",
    "            dq.popleft()\n",
    "        \n",
    "        dq.append(num)\n",
    "    return list(dq)\n"
   ]
  }
